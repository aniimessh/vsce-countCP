{
  "name": "countcp",
  "displayName": "countCP",
  "description": "Counts how many times you copy and paste in VS Code.",
  "version": "1.0.1",
  "publisher": "aniimessh",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Notebooks"
  ],
  "keywords": [
    "copy",
    "paste",
    "tracker"
  ],
  "activationEvents": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/countcp"
  },
  "icon": "images/icon.jpg",
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "countcp.trackCopy",
        "title": "Track Copy Action"
      },
      {
        "command": "countcp.trackPaste",
        "title": "Track Paste Action"
      }
    ],
    "keybindings": [
      {
        "command": "countcp.trackCopy",
        "key": "ctrl+c",
        "when": "editorTextFocus"
      },
      {
        "command": "countcp.trackPaste",
        "key": "ctrl+v",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "ts-loader": "^9.5.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
